{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/app/*": ["./frontend/app/*"],
      "@/models/*": ["./frontend/models/*"],
      "@/components/*": ["./frontend/components/*"],
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "app/contact/page.js", "components/productCard.jsx", "app/api/auth/[...nextauth]/route.js", "libs/mongoConnect.js", "app/profile/page.js", "app/page.jsx", "app/products/page.jsx"],
  "exclude": ["node_modules"]
}
